<include>
    <gateway name="asiatech">
        <param name="username" value="${asiatech_fromuser}"/>
        <param name="password" value="${asiatech_password}"/>
        <param name="proxy" value="voip.asiatech.ir:5060"/>
        <param name="expire-seconds" value="3600"/>
        <param name="register" value="true"/>
        <param name="from-user" value="${asiatech_fromuser}"/>
        <param name="contact-params" value="transport=udp"/>
        <param name="contact-params-invite" value="transport=udp"/>
        <param name="sip-port" value="5060"/>
        <param name="extension" value="${asiatech_fromuser}"/>
        <param name="retry-seconds" value="30"/>
        <param name="context" value="from-trunk"/>
        <param name="register-transport" value="udp"/>
        <param name="caller-id-in-from" value="true"/>
        <param name="inbound-proxy-media" value="true"/>
        <param name="dtmf-type" value="rfc2833"/>
        <param name="sip_cid_type" value="pid"/>
        <param name="auth-method" value="password"/>

    </gateway>
</include>